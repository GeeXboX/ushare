#: src/ushare.c:348
#, c-format
msgid ""
"Usage: ushare [option] [-n name] [-i interface] [-c directory] [[-c "
"directory]...]\n"
msgstr ""
"Usage : ushare [option] [-n nom] [-i interface] [-c répertoire] [[-c "
"répertoire]...]\n"

#: src/ushare.c:349
#, c-format
msgid "Options:\n"
msgstr "Options :\n"

#: src/ushare.c:350
#, c-format
msgid "   -C, --config=FILE \t\tUse FILE as config file (default is '%s')\n"
msgstr ""
"   -C, --config=FICHIER \tUtiliser FICHIER comme fichier de configuration ('%"
"s' par défaut)\n"

#: src/ushare.c:351
#, c-format
msgid "   -n, --name=NAME \t\tSet UPnP Friendly Name (default is '%s')\n"
msgstr "   -n, --name=NOM \t\tDéfini le nom UPnP ('%s' par défaut)\n"

#: src/ushare.c:352
#, c-format
msgid ""
"   -i, --interface=IFACE \tUse IFACE Network Interface (default is '%s')\n"
msgstr ""
"   -i, --interface=IFACE \tUtiliser l'interface réseau IFACE ('%s' par "
"défaut)\n"

#: src/ushare.c:353
#, c-format
msgid ""
"   -c, --content=DIR \t\tShare the content of DIR directory (default is "
"'./')\n"
msgstr ""
"   -c, --content=REP \t\tPartage le contenu du répertoire REP ('./' par "
"défaut)\n"

#: src/ushare.c:354
#, c-format
msgid "   -v, --verbose \t\tSet verbose display\n"
msgstr "   -v, --verbose \t\tMode bavard\n"

#: src/ushare.c:355
#, c-format
msgid "   -V, --version \t\tDisplay the version of uShare and exit\n"
msgstr ""
"   -V, --version \t\tAffiche la version de uShare et quitte le programme\n"

#: src/ushare.c:356
#, c-format
msgid "   -h, --help \t\t\tDisplay this help\n"
msgstr "   -h, --help \t\t\tAffiche cette aide\n"

#: src/ushare.c:455
#, c-format
msgid "Error: file \"%s\" doesn't exist, using default file\n"
msgstr ""
"Erreur : le fichier \"%s\" n'existe pas, utilisation du fichier par défaut\n"

