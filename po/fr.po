#: src/ushare.c:329
#, c-format
msgid ""
"Usage: ushare [option] [-n name] [-i interface] [-c directory] [[-c "
"directory]...]\n"
msgstr ""
"Usage : ushare [option] [-n nom] [-i interface] [-c répertoire] [[-c "
"répertoire]...]\n"

#: src/ushare.c:330
#, c-format
msgid "Options:\n"
msgstr "Options :\n"

#: src/ushare.c:331
#, c-format
msgid "   -n, --name=NAME \t\tSet UPnP Friendly Name (default is 'uShare')\n"
msgstr "   -n, --name=NOM \t\tDéfini le nom UPnP ('uShare' par défaut)\n"

#: src/ushare.c:332
#, c-format
msgid ""
"   -i, --interface=IFACE \tUse IFACE Network Interface (default is 'eth0')\n"
msgstr ""
"   -i, --interface=IFACE \tUtiliser l'interface réseau IFACE ('eth0' par "
"défaut)\n"

#: src/ushare.c:333
#, c-format
msgid ""
"   -c, --content=DIR \t\tShare the content of DIR directory (default is "
"'./')\n"
msgstr ""
"   -c, --content=REP \t\tPartage le contenu du répertoire REP ('./' par "
"défaut)\n"

#: src/ushare.c:334
#, c-format
msgid "   -v, --version \t\tDisplay the version of uShare and exit\n"
msgstr ""
"   -v, --version \t\tAffiche la version de uShare et quitte le programme\n"

#: src/ushare.c:335
#, c-format
msgid "   -h, --help \t\t\tDisplay this help\n"
msgstr "   -h, --help \t\t\tAffiche cette aide\n"

