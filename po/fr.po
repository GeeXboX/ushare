#: src/ushare.c:326
#, fuzzy, c-format
msgid ""
"\n"
"Options :\n"
msgstr ""
"\n"
"Options :\n"

#: src/ushare.c:327
#, c-format
msgid "   -n, --name :\t\tSet UPnP Friendly Name (default is 'uShare')\n"
msgstr "   -n, --name :\t\tDéfini le nom UPnP ('uShare' par défaut)\n"

#: src/ushare.c:328
#, c-format
msgid "   -i, --interface :\tSet Network Interface (default is 'eth0')\n"
msgstr ""
"   -i, --interface :\tDéfini le nom de l'interface ('eth0' par défaut)\n"

#: src/ushare.c:329
#, c-format
msgid ""
"   -c, --content :\tSet the content directory to be shared (default is "
"'./')\n"
msgstr ""
"   -c, --content :\tDéfini le répertoire de contenu à partager ('./' par "
"défaut)\n"

#: src/ushare.c:330
#, c-format
msgid "   -v, --version :\tDisplay the version of uShare and exit.\n"
msgstr ""
"   -v, --version :\tAffiche la version de uShare et quitte le programme.\n"

#: src/ushare.c:331
#, c-format
msgid "   -h, --help :\t\tDisplay this help\n"
msgstr "   -h, --help :\t\tAffiche cette aide\n"

